{
  "name": "D-D-Studio-Website",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/Diama1/Studio-Album.git",
  "author": "Diama1 <32199791+Diama1@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --exec babel-node src/index.js",
    "start": "yarn run migrate && node dist/index.js",
    "test": "nyc --reporter=html --reporter=text mocha ./tests/*.js --exit --require @babel/register --require regenerator-runtime --timeout 3000",
    "test:reset": "npm run migrate && nyc --reporter=html --reporter=text mocha ./tests/*.js --exit --require @babel/register --require regenerator-runtime --timeout 10000",
    "devtest": "NODE_ENV=test nyc --reporter=html --reporter=text mocha ./tests/*.js --exit --require @babel/register --require regenerator-runtime --timeout 3000",
    "migrate": "sequelize db:migrate",
    "migrate:reset": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "babel src --out-dir dist"
  },
  "dependencies": {
    "@hapi/joi": "^15.0.3",
    "cloudinary": "^1.14.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "multer": "^1.4.2",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.19.0",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.6",
    "mocha": "^6.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.2",
    "nyc": "^14.1.1"
  }
}
